              </div>
              <div class="password">
                <div class="rectangle-62"></div>
                <label for="password-admin" class="password2">Password</label>
                <input type="password" id="password-admin" name="password-admin" required>
              </div>
          </form>
        </div>
      </div>
      <div class="frame-427319727">
        <div class="logo">LOGO</div>
      </div>
    </div>
    <script>
        function redirctToCreateViewUser() {
            window.location.href = '/create-view-user';
        }
    </script>
